{
  "appName": "dewey-decimal",
  "databaseName": "dewey.Database",
  "databaseScriptName": "createDatabase.sql",
  "transitionDuration": 400,
  "fadeTransitionDuration": 400,
  "slideTransitionDuration": 400,
  "barcode": {
    "format": "CODE128",
    "width": 2,
    "height": 100
  },
  "tables": {
    "author": {
      "pk": "author_id",
      "name": "author"
    },
    "dewey_summary_2": {
      "pk": "dewey_summary_2_id",
      "name": "dewey_summary_2"
    },
    "dewey_summary_3": {
      "pk": "dewey_summary_3_id",
      "name": "dewey_summary_3"
    },
    "dewey_decimal": {
      "pk": "dewey_decimal_id",
      "name": "dewey_decimal"
    },
    "book": {
      "pk": "book_id",
      "name": "book"
    },
    "books_out": {
      "pk": "books_out_id",
      "name": "books_out"
    },
    "student": {
      "pk": "student_id",
      "name": "student"
    }
  },
  "logs": {
    "error": "dewey.error.log",
    "general": "dewey.general.log"
  },
  "checkout": {
    "daysAllowedOut": 5,
    "isBusinessDays": true
  },
  "fines": {
    "isEnabled": true,
    "rate": 5
  },
  "formatDate": {
    "from": "yyyy-MM-dd",
    "to": "dd MMM yyyy",
    "errorLog": {
      "from": "yyyy-MM-dd HH:mm:ss",
      "to": "dd MMM yyyy h:mm:ss aaaa"
    }
  },
  "databaseLocation": "D:\\Playground\\Electron\\di\\dewey-decimal-app\\dewey.Database"
}
